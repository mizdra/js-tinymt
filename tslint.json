{
  "extends": [
    "tslint:recommended",
    "tslint-config-standard"
  ],
  "rules": {
    "interface-name": false,
    "no-console": { "severity": "warning" },
    "ordered-imports": true,
    "indent": [true, "spaces", 2],
    "trailing-comma": [true, {"multiline": "always", "singleline": "never"}]
  }
}
